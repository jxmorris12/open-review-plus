{
  "name": "open-review-plus",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Enhancements to OpenReview",
  "icons" : {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.openreview.net/forum*"],
      "js": ["src/jquery-3.4.1.js", "src/open-review-plus.js"],
      "css": ["src/open-review-plus.css"]
    }
  ]

}
